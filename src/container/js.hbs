{{!-- 
Це код container файлу для js сторінки, 
в якому міститься код, котрий буде віддаватися по шляху (PATH) - "/js"
--}}

<div>
     <section>
           <h1>{{name}}</h1>
           <br>
           <p>{{description}}</p>
           <time>{{history.year}}</time>
           <h5> {{history.founder}}</h5>
     </section>
     <hr>
     <section>
         <h5>Types:</h5>
         <ul>
            {{#each types}}
             <li>
               {{[]}}
               </li>
             {{/each}}
         </ul>
         <br>
     </section>
     <hr>
     <section>
         <h5>Syntax:</h5>
         <ul>
            {{#with syntax}}
             <li>variables:{{#each variables}} "{{[]}}" ; {{/each}} </li>
             <li>functions:{{#each functions}} "{{[]}}" ; {{/each}} </li>
             <li>conditionals:{{#each conditionals}} "{{[]}}" ; {{/each}} </li>
             <li>loops:{{#each loops}} "{{[]}}" ; {{/each}} </li>
             <li>classes:{{#each classes}} "{{[]}}" ; {{/each}} </li>
             {{/with}}
         </ul>
         <br>
     </section>
          <hr>
     <section>
         <h5>Features:</h5>
         <ul>
            {{#each features}}
             <li>
               {{[]}}
               </li>
             {{/each}}
         </ul>
         <br>
     </section>
          <hr>
     <section>
         <h5>Libraries:</h5>
         <ul>
            {{#each libraries}}
             <li>
               {{[]}}
               </li>
             {{/each}}
         </ul>
         <br>
     </section>
     <hr>
     <section>
         <h5>Tools:</h5>
         <ul>
            {{#each tools}}
             <li>
               {{[]}}
               </li>
             {{/each}}
         </ul>
         <br>
     </section>
     <hr>
     <section>
         <h5>Community:</h5>
         <ul>
            {{#each community}}
             <li>
               "{{name}}"({{type}})
               </li>
             {{/each}}
         </ul>
         <br>
     </section>
</div>

